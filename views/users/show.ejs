<h1><%= title %></h1>
<table id="user-data" class="user-ovw">
  <tr>
    <th>Name</th>
    <td id="user-name"></td>
  </tr>
  <tr>
    <th>Address</th>
    <td id="user-address"></td>
  </tr>
  <tr>
    <th>Email</th>
    <td id="user-email"></td>
  </tr>
  <tr>
    <th>Role</th>
    <td id="user-role"></td>
  </tr>
  <tr>
    <th>apiToken</th>
    <td id="user-apiToken"></td>
  </tr>
</table>
<a href="/users"> <button class="button" type="submit">View users </button></a>
<a href="<%= `/users/${user._id}/edit` %>">
  <button type="submit" class="button">Edit</button>
</a>

<a href="<%= `/users/${user._id}/delete?_method=DELETE` %>" onclick="return confirm('Are you sure you want to delete this record?')">
  <button type="submit" class="button">Delete</button>
</a>
 

