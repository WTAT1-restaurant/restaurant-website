<html lang="en">
<head>
    <meta charset="UTF-8">
    <link rel="stylesheet" type="text/css" href="/css/style.css">
    <title>Profile</title>
</head>
<body>
    <h1><%= user.fullName %>'s Profile</h1>
    <table class="table">
        <tr>
            <th>Name</th>
            <td><%= user.fullName %></td>
        </tr>
        <tr>
            <th>Zip Code</th>
            <td><%= user.zipCode %></td>
        </tr>
        <tr>
            <th>Email</th>
            <td><%= user.email %></td>
        </tr>
        <tr>
            <th>Password</th>
            <td><%= user.password %></td>
        </tr>
    </table>
</body>
</html>